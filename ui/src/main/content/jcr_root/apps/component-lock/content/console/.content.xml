<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content
            jcr:primaryType="nt:unstructured"
            jcr:title="Component Lock"
            sling:redirect="false"
            sling:resourceType="granite/ui/components/foundation/page"
            sling:vanityOrder="1200"
            sling:vanityPath="/component-lock">
        <head jcr:primaryType="nt:unstructured">
            <contentchecker jcr:primaryType="nt:unstructured"
                            alowEmpty="false"
                            path="${requestPathInfo.suffix}"
                            redirect="/projects.html"
                            sling:resourceType="granite/ui/components/foundation/admin/contentchecker"/>
            <viewport jcr:primaryType="nt:unstructured"
                      sling:resourceType="granite/ui/components/foundation/admin/page/viewport"/>
            <meta jcr:primaryType="nt:unstructured"
                  content="chrome=1"
                  name="X-UA-Compatible"
                  sling:resourceType="granite/ui/components/foundation/meta"/>
            <title jcr:primaryType="nt:unstructured"
                   jcr:title="Component Lock Console"
                   sling:resourceType="cq/gui/components/projects/admin/page/title"/>
            <favicon jcr:primaryType="nt:unstructured"
                     sling:resourceType="granite/ui/components/foundation/page/favicon"/>
            <clientlibs jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/includeclientlibs"
                        categories="[granite.ui.foundation,granite.ui.foundation.admin,cq.projects.admin,cq.projects.admin.history.timeline,cq.gui.siteadmin.admin.pages,component-lock.console]"/>
        </head>
        <body jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/foundation/page/body">
            <layout jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/endor/layouts/page"/>
            <rail jcr:primaryType="nt:unstructured"
                  currentId="component-lock-console"
                  sling:resourceType="granite/ui/components/endor/navcolumns"
                  srcPath="cq/core/content/nav"/>
            <breadcrumbs jcr:primaryType="nt:unstructured"
                         sling:resourceType="cq/gui/components/projects/admin/endor/breadcrumbs"/>
            <title jcr:primaryType="nt:unstructured"
                   jcr:title="Component Lock Console"
                   sling:resourceType="cq/gui/components/projects/admin/endor/title"/>
            <helpactivator jcr:primaryType="nt:unstructured"
                           href="#help-popover"
                           sling:resourceType="granite/ui/components/foundation/hyperlink"
                           text="Help"
                           toggle="popover"/>
            <badge jcr:primaryType="nt:unstructured"
                   href="#cq-notifications"
                   sling:resourceType="cq/gui/components/endor/badge"/>
            <user jcr:primaryType="nt:unstructured"
                  href="#granite-user-properties"
                  sling:resourceType="granite/ui/components/endor/user"/>
            <help jcr:primaryType="nt:unstructured"
                  id="help-popover"
                  sling:resourceType="granite/ui/components/foundation/container">
                <layout jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/layouts/popover"/>
                <items jcr:primaryType="nt:unstructured">
                    <list jcr:primaryType="nt:unstructured"
                          sling:resourceType="granite/ui/components/foundation/container">
                        <layout jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/layouts/list"/>
                        <items jcr:primaryType="nt:unstructured">
                            <searchform jcr:primaryType="nt:unstructured"
                                        action="http://www.adobe.com/cfusion/search/index.cfm"
                                        sling:resourceType="granite/ui/components/foundation/form">
                                <layout jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/foundation/layouts/form"/>
                                <items jcr:primaryType="nt:unstructured">
                                    <section jcr:primaryType="nt:unstructured"
                                             name="siteSection"
                                             sling:resourceType="granite/ui/components/foundation/form/hidden"
                                             value="marketing-cloud.html"/>
                                    <term jcr:primaryType="nt:unstructured"
                                          emptyText="Search"
                                          name="term"
                                          sling:resourceType="granite/ui/components/foundation/form/textfield"/>
                                </items>
                            </searchform>
                            <whatsnew jcr:primaryType="nt:unstructured"
                                      href_i18n="https://microsite.omniture.com/t2/help/en_US/whatsnew/"
                                      sling:resourceType="granite/ui/components/foundation/hyperlink"
                                      text="What's New">
                                <layoutConfig jcr:primaryType="nt:unstructured"
                                              interactive="true"/>
                            </whatsnew>
                            <aemdoc jcr:primaryType="nt:unstructured"
                                    href_i18n="https://www.adobe.com/go/aem6_docs"
                                    sling:resourceType="granite/ui/components/foundation/hyperlink"
                                    text="Documentation">
                                <layoutConfig jcr:primaryType="nt:unstructured"
                                              interactive="true"/>
                            </aemdoc>
                            <home jcr:primaryType="nt:unstructured"
                                  href_i18n="http://microsite.omniture.com/t2/help/en_US/home/"
                                  sling:resourceType="granite/ui/components/foundation/hyperlink"
                                  text="Help Home">
                                <layoutConfig jcr:primaryType="nt:unstructured"
                                              interactive="true"/>
                            </home>
                            <community jcr:primaryType="nt:unstructured"
                                       href_i18n="http://helpx.adobe.com/marketing-cloud.html"
                                       sling:resourceType="granite/ui/components/foundation/hyperlink"
                                       text="Community">
                                <layoutConfig jcr:primaryType="nt:unstructured"
                                              interactive="true"/>
                            </community>
                            <customercare jcr:primaryType="nt:unstructured"
                                          href_i18n="http://helpx.adobe.com/marketing-cloud/contact-support.html"
                                          sling:resourceType="granite/ui/components/foundation/hyperlink"
                                          text="Customer Care">
                                <layoutConfig jcr:primaryType="nt:unstructured"
                                              interactive="true"/>
                            </customercare>
                        </items>
                    </list>
                </items>
            </help>
            <notifications jcr:primaryType="nt:unstructured"
                           id="cq-notifications"
                           sling:resourceType="cq/gui/components/endor/notifications"/>
            <account jcr:primaryType="nt:unstructured"
                     path="granite/ui/content/userproperties"
                     sling:resourceType="granite/ui/components/foundation/include"/>
            <content jcr:primaryType="nt:unstructured"
                     sling:resourceType="granite/ui/components/foundation/container">
                <layout jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/endor/layouts/content"/>
                <aside jcr:primaryType="nt:unstructured"
                       sling:resourceType="granite/ui/components/foundation/container">
                    <layout jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/endor/layouts/sidepanels"/>
                    <items jcr:primaryType="nt:unstructured">
                        <timeline jcr:primaryType="nt:unstructured"
                                  id="cq-rail-timeline"
                                  sling:resourceType="cq/gui/components/common/admin/timeline">
                            <items jcr:primaryType="nt:unstructured">
                                <alerts jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/common/admin/timeline/alerts">
                                    <items jcr:primaryType="nt:unstructured">
                                        <workflows jcr:primaryType="nt:unstructured"
                                                   sling:resourceType="cq/gui/components/common/admin/timeline/alerts/workflow"/>
                                    </items>
                                </alerts>
                                <filter jcr:primaryType="nt:unstructured"
                                        class="cq-common-admin-timeline-filter"
                                        emptyText="Recently used"
                                        noWidth="true"
                                        rel="cq-common-admin-timeline-filter"
                                        sling:resourceType="granite/ui/components/foundation/form/select"
                                        text="Recently used">
                                    <items jcr:primaryType="nt:unstructured">
                                        <default jcr:primaryType="nt:unstructured"
                                                 sling:resourceType="granite/ui/components/foundation/form/option"
                                                 text="Recently used"
                                                 value="all"/>
                                        <view jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/foundation/form/option"
                                              text="Last viewed"
                                              value="history:VIEW"/>
                                        <edit jcr:primaryType="nt:unstructured"
                                              sling:resourceType="granite/ui/components/foundation/form/option"
                                              text="Last edited"
                                              value="history:EDIT"/>
                                    </items>
                                </filter>
                                <events jcr:primaryType="nt:unstructured"
                                        sling:resourceType="cq/gui/components/common/admin/timeline/events">
                                    <notifications jcr:primaryType="nt:unstructured">
                                        <empty jcr:primaryType="nt:unstructured"
                                               jcr:title="Select a project to display its timeline."/>
                                        <unavailable jcr:primaryType="nt:unstructured"
                                                     jcr:title="The timeline is not available for project details."/>
                                    </notifications>
                                    <providers jcr:primaryType="nt:unstructured">
                                        <history jcr:primaryType="nt:unstructured"
                                                 filter="history"
                                                 name="com.adobe.cq.projects.impl.history.ProjectHistoryTimelineEventProvider$1"
                                                 sling:resourceType="cq/gui/components/projects/admin/historytimeline"/>
                                    </providers>
                                </events>
                            </items>
                        </timeline>
                    </items>
                </aside>
